{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "age": { "type": "number" },
    "occupation": { "enum": ["doctor", "teacher", "engineer"] },
    "id": { "type": "integer", "minimum": 0 }
  },
  "required": ["id", "name"]
}
